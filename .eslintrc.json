{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "trailingComma": "none",
        "singleQuote": true,
        "printWidth": 100,
        "semi": true
      }
    ]
  }
}
